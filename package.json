{
  "name": "realty",
  "version": "1.0.0",
  "description": "Realty monorepo",
  "main": "index.js",
  "workspaces": [
    "realty-app",
    "realty-server"
  ],
  "scripts": {
    "commit": "cz",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install",
    "back:dev": "cd realty-server && npm run start:dev",
    "front:dev": "cd realty-app && npm run dev",
    "start": "npm-run-all --parallel back:dev front:dev"
  },
  "keywords": [
    "realty",
    "typescript",
    "Nest.js",
    "Next.js",
    "prisma"
  ],
  "author": "RÃ³mulo Corona",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@s-ui/mono": "^2.34.0",
    "@types/passport-local": "^1.0.35",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "prisma": "^4.11.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@nestjs/passport": "^9.0.3",
    "@prisma/client": "^4.11.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0"
  }
}
